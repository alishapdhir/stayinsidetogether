{
  "manifest_version": 2,
  "name": "Stay Inside Together",
  "version": "1.0",
  "description": "trying is winning",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
